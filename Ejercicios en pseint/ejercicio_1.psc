Proceso ejercicio_1
	definir a,b,c como reales;
	definir resultado Como Real;
	escribir "Dele un valor a A: ";
	leer a;
	escribir "Dele un valor a B: ";
	leer b;
	escribir "Dele un valor a C: ";
	leer c;
	
	resultado <- (-b+rc(b^2-4*a*c))/(2*a);
	
	escribir "El resultado es: ",resultado;
FinProceso
